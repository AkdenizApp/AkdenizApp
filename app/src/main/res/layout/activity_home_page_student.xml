<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".HomePageStudent" android:orientation="vertical" android:gravity="center|top">

    <TextView
            android:text="Akdeniz University"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:textSize="24sp" tools:layout_editor_absoluteY="197dp" tools:layout_editor_absoluteX="88dp"
            tools:ignore="MissingConstraints" android:layout_margin="20dp"/>
    <androidx.gridlayout.widget.GridLayout
            android:layout_width="348dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="300dp"
            tools:layout_editor_absoluteX="72dp"
            app:columnCount="4" tools:ignore="MissingConstraints" app:rowCount="8">

        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                app:layout_row="0"
                app:layout_column="0"
                android:background="@drawable/bullhorn"
                android:id="@+id/announcements_button"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_marginLeft="10dp"
                app:layout_row="0"
                android:background="@drawable/calendar"
                app:layout_column="1"
                android:id="@+id/events"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/bubbles2"
                android:layout_marginLeft="10dp"
                app:layout_row="0"
                app:layout_column="2"
                android:id="@+id/complaints_button"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/map"
                android:layout_marginLeft="10dp"
                android:id="@+id/zerothree" app:layout_row="0" app:layout_column="3"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/buss"
                android:id="@+id/shuttle" app:layout_row="2" app:layout_column="0"
                android:layout_marginTop="10dp"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/pushpin"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:id="@+id/feedback" app:layout_row="2" app:layout_column="1"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/books"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"

                android:id="@+id/classes" app:layout_row="2" app:layout_column="2"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/spoonknife"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:id="@+id/dining" app:layout_row="2" app:layout_column="3"
                app:layout_gravity="center_horizontal"/>
        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/clubs"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:id="@+id/clubs" app:layout_row="4" app:layout_column="1"
                app:layout_gravity="center_horizontal"/>

        <ImageButton
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="@drawable/question"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:id="@+id/survey" app:layout_row="4" app:layout_column="2"
                app:layout_gravity="center_horizontal"/>

        <TextView
                android:text="Duyurular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="1" app:layout_column="0"
                android:id="@+id/textView" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Etkinlikler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="1" app:layout_column="1"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView2" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Şikayetler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="1" app:layout_column="2"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView4" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Okul Haritası"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="1" app:layout_column="3"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView5" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Shuttle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="3" app:layout_column="0"
                android:id="@+id/textView6" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Geri Bildirim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="3" app:layout_column="1"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView7" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Sınıflar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="3" app:layout_column="2"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView8" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Yemekhane"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="3" app:layout_column="3"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView9" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Kulüpler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="5" app:layout_column="1"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView10" app:layout_gravity="center_horizontal"/>
        <TextView
                android:text="Anketler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_row="5" app:layout_column="2"
                android:layout_marginLeft="10dp"
                android:id="@+id/textView12" app:layout_gravity="center_horizontal"/>

    </androidx.gridlayout.widget.GridLayout>


</LinearLayout>